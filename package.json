{
  "name": "create-ai-kit",
  "version": "1.7.0",
  "description": "Scaffold AI Kit - AI-friendly documentation and workflow system for Cursor IDE",
  "bin": {
    "create-ai-kit": "bin/create-ai-kit.js"
  },
  "scripts": {
    "test": "node --test test/*.test.js",
    "preview": "node bin/create-ai-kit.js --dry-run",
    "format": "prettier --write .",
    "release:check": "echo '\\nðŸ“‹ Before releasing, complete PRE-RELEASE-CHECKLIST.md\\n' && echo 'Steps:\\n  1. Review PRE-RELEASE-CHECKLIST.md\\n  2. Run: npm test\\n  3. Run: npm audit\\n  4. Run: npm pack --dry-run\\n  5. Publish: npm publish\\n'",
    "docs:update": "node scripts/docs-update/generate-context.js",
    "docs:check": "node scripts/docs-update/check-markers.js",
    "docs:check:ci": "node scripts/docs-update/check-markers.js --ci",
    "docs:verify-inline": "node scripts/docs-update/verify-inline.js"
  },
  "keywords": [
    "ai-kit",
    "cursor",
    "ai",
    "scaffold",
    "documentation",
    "workflow",
    "cli",
    "developer-tools"
  ],
  "author": "Bohdan Kolesnik <internium.founder@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/interniums/create-ai-kit.git"
  },
  "bugs": {
    "url": "https://github.com/interniums/create-ai-kit/issues"
  },
  "homepage": "https://github.com/interniums/create-ai-kit#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "fs-extra": "^10.0.0",
    "picomatch": "^4.0.2"
  },
  "optionalDependencies": {
    "clipboardy": "^2.3.0"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "prettier": "^3.2.0"
  },
  "files": [
    "bin/",
    "templates/",
    "test/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ]
}
